{
	"compilerOptions": {
		"baseUrl": "./src",
		"paths": {
			"@components": ["components/index"],
			"@components/*": ["components/*"],
			"@constants": ["constants/index"],
			"@constants/*": ["constants/*"],
			"@context": ["context/index"],
			"@context/*": ["context/*"],
			"@data": ["data/index"],
			"@data/*": ["data/*"],
			"@fb": ["firebase/index"],
			"@fb/*": ["firebase/*"],
			"@hooks": ["hooks/index"],
			"@hooks/*": ["hooks/*"],
			"@lotties": ["lottie/index"],
			"@lotties/*": ["lottie/*"],
			"@models": ["models/index"],
			"@models/*": ["models/*"],
			"@api": ["api/index"],
			"@api/*": ["api/*"],
			"@atom": ["atom/index"],
			"@atom/*": ["atom/*"],
			"@theme/*": ["theme/*"],
			"@theme": ["theme/index"],
			"@cookies": ["cookies/index"],
			"@cookies/*": ["cookies/*"],
			"@styles": ["styles/index"],
			"@styles/*": ["styles/*"],
			"@lib": ["lib/index"],
			"@lib/*": ["lib/*"],
			"@store": ["store/index"],
			"@store/*": ["store/*"]
		}
	}
}
